
fun tabliczka( a , b , c )
{
    var tablica = [ pow(a) * 2 , pow(b) * 2 , pow(c) * 2 ];
    return tablica;
}

fun sumka( a , b , c )
{
    var result = a + b + c;
    return result;
}


fun pow(x)
{
    var result = x * x;

    for var i = 0; i < 100; i += 1 {
        result += i;
    }

    return result;
}

var tablica = tabliczka( 20, 30, 40 );
var pierwszy = tablica[0];

for var i = 0; i < 10; i += 1 {
    println(pow(i));
}

println(pierwszy);