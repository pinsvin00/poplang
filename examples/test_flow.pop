var primes = [ 2, 3, 5, 7 ];

for var i = 8 ; i < 100; i += 1 {
    var is_prime = true;
    for var j = 0; j < primes.size; j += 1 {
        if ( i % primes[j] ) == 0 {
            is_prime = false;
            break;
        }
    }

    if is_prime {
        primes.append(i);
    }
}

print "The prime numbers are";
for var i = 0; i < primes.size; i += 1 {
    print primes[i];
}


